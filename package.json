    "@types/ioredis": "^4.28.10",
    "@types/three": "^0.175.0",
    "axios": "^1.8.4",
    "babel-loader": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.5",
    "chart.js": "^4.4.8",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "copy-webpack-plugin": "^13.0.0",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dompurify": "^3.2.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "file-loader": "^6.2.0",
