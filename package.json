    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": "20.10.0",
    "npm": "9.8.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/client/src/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/client/src/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/client/src/setupTests.js"
    ]
  }
}
