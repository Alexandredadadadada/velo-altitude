{
  "name": "velo-altitude",
  "version": "1.0.0",
  "description": "Plateforme complète dédiée au cyclisme de montagne en Europe",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "nodemon ./server/server.js",
    "build": "npx webpack --config webpack.config.js --mode production --stats-error-details",
    "build:analyze": "GENERATE_BUNDLE_REPORT=true npm run build && npm run analyze",
    "analyze": "webpack-bundle-analyzer --port 8888 build/bundle-stats.json",
    "prebuild": "npm run clean",
    "lint": "eslint .",
    "test": "jest",
    "check-deps": "node ./scripts/check-dependencies.js",
    "netlify-build": "npm install && npm run build",
    "test:performance": "node performance-testing/e2e-performance-tests.js",
    "test:lighthouse": "lhci autorun --config=performance-testing/lighthouse-ci-config.js",
    "performance:report": "npm run build && npm run test:lighthouse && npm run test:performance"
  },
  "keywords": [
    "cyclisme",
    "europe",
    "vélo",
    "montagne",
    "cols",
    "itinéraires",
    "météo",
    "strava",
    "altitude"
  ],
  "author": "Velo-Altitude Team",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
