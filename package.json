{
  "name": "velo-altitude",
  "version": "1.0.0",
  "description": "Plateforme complète dédiée au cyclisme de montagne en Europe",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "nodemon ./server/server.js",
    "clean": "node scripts/clean.js",
    "build": "npx webpack --config webpack.config.js --mode production",
    "build:analyze": "GENERATE_BUNDLE_REPORT=true npm run build && npm run analyze",
    "analyze": "webpack-bundle-analyzer --port 8888 build/bundle-stats.json",
    "prebuild": "node scripts/netlify-prebuild.js",
    "postbuild": "node scripts/post-build.js",
    "lint": "eslint .",
    "test": "jest",
    "check-deps": "node ./scripts/check-dependencies.js",
    "build:prod": "node scripts/build-prod.js",
    "build:netlify": "cross-env CI=false GENERATE_SOURCEMAP=false npm run build",
    "build:netlify-fix": "node scripts/build-netlify-fix.js",
    "build:netlify-specific": "node scripts/build-netlify-specific.js",
    "test:netlify-build": "node scripts/test-netlify-build.js",
    "build:minimal": "node scripts/build-minimal.js",
    "build:hybrid": "node scripts/build-hybrid.js",
    "build:complete": "node scripts/build-complete.js",
    "build:fast": "npx webpack --config webpack.config.js --mode production --no-progress",
    "test:performance": "node performance-testing/e2e-performance-tests.js",
    "test:lighthouse": "lhci autorun --config=performance-testing/lighthouse-ci-config.js",
    "performance:report": "npm run build && npm run test:lighthouse && npm run test:performance",
    "verify-deploy": "node scripts/verify-deployment.js",
    "verify-netlify": "node scripts/verify-netlify-build.js",
    "regenerate-cols": "ts-node scripts/regenerate-cols.ts",
    "regenerate-cols-now": "ts-node scripts/run-regeneration-now.ts",
    "regenerate-now": "node scripts/run-regeneration-now.js",
    "regenerate-from-challenges": "ts-node src/scripts/run-regeneration.ts",
    "regenerate-cols-js": "node src/scripts/run-regeneration.js",
    "regenerate-direct": "node scripts/regenerate-profiles-direct.js",
    "validate": "ts-node scripts/run-validation.ts",
    "test:weather": "ts-node src/tests/manual/weather-verification.ts listAllTests",
    "test:weather:run": "ts-node src/tests/manual/weather-verification.ts",
    "verify:deps": "ts-node scripts/verify-dependencies.ts",
    "pre-deploy": "npm run validate && npm run build:prod"
  },
  "keywords": [
    "cyclisme",
    "europe",
    "vélo",
    "montagne",
    "cols",
    "itinéraires",
    "météo",
    "strava",
    "altitude"
  ],
  "author": "Velo-Altitude Team",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@mui/icons-material": "^5.17.1",
