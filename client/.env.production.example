# Production environment variables
REACT_APP_API_URL=https://api.velo-altitude.com
REACT_APP_AUTH0_DOMAIN=velo-altitude.eu.auth0.com
REACT_APP_AUTH0_CLIENT_ID=actual-production-client-id
REACT_APP_AUTH0_AUDIENCE=https://api.velo-altitude.com
REACT_APP_AUTH0_SCOPE=openid profile email

# Disable development-only features
REACT_APP_ENABLE_MSW=false
REACT_APP_AUTH0_DEV_MODE=false

# Enable performance monitoring in production
REACT_APP_DISABLE_PERFORMANCE_MONITORING=false
