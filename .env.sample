# API Keys and Secrets
OPENWEATHER_API_KEY=your_openweather_api_key
MAPBOX_TOKEN=your_mapbox_token
OPENROUTE_API_KEY=your_openroute_api_key
WINDY_PLUGINS_API=your_windy_api_key

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/velo-altitude
MONGODB_DB_NAME=velo-altitude

# Auth0 Configuration
AUTH0_AUDIENCE=your_auth0_audience
AUTH0_BASE_URL=your_auth0_base_url
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret
AUTH0_ISSUER_BASE_URL=your_auth0_issuer_base_url
AUTH0_SCOPE=your_auth0_scope
AUTH0_SECRET=your_auth0_secret

# React App Environment Variables
REACT_APP_API_URL=https://velo-altitude.com/api
REACT_APP_BASE_URL=https://velo-altitude.com
REACT_APP_BRAND_NAME=Velo-Altitude
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_VERSION=1.0.0
REACT_APP_MAPBOX_TOKEN=${MAPBOX_TOKEN}

# Strava API
STRAVA_ACCESS_TOKEN=your_strava_access_token
STRAVA_CLIENT_ID=your_strava_client_id
STRAVA_CLIENT_SECRET=your_strava_client_secret
STRAVA_REFRESH_TOKEN=your_strava_refresh_token

# App Configuration
NODE_ENV=development
API_KEYS_ENCRYPTION_KEY=your_encryption_key
SESSION_SECRET=your_session_secret

# Performance and Optimization
ENABLE_BROTLI_COMPRESSION=true
ASSET_CACHE_MAX_AGE=31536000
